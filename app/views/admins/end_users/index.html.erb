<div class="container">
  <div class="row">
    <div class="col col-md-10 col-lg-8 mx-auto">
      <table class="table table-hover">
        <thead>
          <tr>
            <td>ユーザーニックネーム</td>
            <td>ユーザーid</td>
            <td>ユーザーprofile_pic</td>
          </tr>
        </thead>
        <% @endUsers.each do |endUser| %>
        <tbody>
          <tr>
            <td><%= endUser.nickname %></td>
            <td>id:<%= endUser.id %></td>
            <td>
              <%= link_to admins_end_user_path(endUser) do %>
                <%= attachment_image_tag endUser, :profile_pic,:fill, 80, 80, class:"rounded-circle" %>
              <% end %>
            </td>
          </tr>
        </tbody>
        <% end %>
      </table>
    </div>
  </div>
</div>