<% unless end_user == current_end_user %>
  <% if current_end_user.following?(end_user) %>
    <%= link_to "フォロー中", unfollow_path(end_user), method: :post, remote: true, class: "btn btnTransparence pt-3 relationship-btn" %>
  <% else %>
    <%= link_to "フォローする", follow_path(end_user), method: :post, remote: true, class:"btn btnBlueDark pt-3 relationship-btn" %>
  <% end %>
<% end %>
