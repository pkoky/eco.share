<div class="container my-5">
  <div class="row">
    <div class="col-md-8 col-lg-8">
      <% @end_users.each do |end_user| %>
      <table class="table table-bordered table-condensed mt-3">
        <tbody>
          <tr id="follow_<%= end_user.id %>">
            <td>
              <p>id:<%= end_user.id %></p>
              <%= link_to end_user_path(end_user.id) do %>
                <%= end_user.nickname %>
              <% end %>
              <p class="relationship">
                <%= render "end_user/relationships/relationship", end_user: end_user %>
              </p>
            </td>
          </tr>
          <tr>
            <td>
              <%= end_user.self_introduction %>
            </td>
          </tr>
        </tbody>
      </table>
      <% end %>
    </div>
  </div>
</div>